            <string>///Movement
if (Attacking = false){
    //Poruszanie na dol
    if(keyboard_check(ord('S')) &amp;&amp; (place_free(x,y+4))){
    //jesli gracz wcisnie 's' i beda dwa miejsca w na vektor y wolne to zrob 
        y+=2;
        sprite_index = spr_Player_Walk_Down; 
        //przypisanie animacji do obiektu bo obiekt robi sie z jednej klatk
        image_speed = 0.5;
        //zwolnienie animacji bo zapieprzala jak szalona
        Direction = "Down";
    }
    //Poruszanie na gore
    if(keyboard_check(ord('W')) &amp;&amp; (place_free(x,y-4))){
        y-=2;
        sprite_index = spr_Player_Walk_Up; 
        image_speed = 0.5;
        Direction = "Up";
    }
    if(keyboard_check(ord('D')) &amp;&amp; (place_free(x+4,y))){
        x+=2;
        sprite_index = spr_Player_Walk_Right; 
        image_speed = 0.5;
        Direction = "Right";
    }
    if(keyboard_check(ord('A')) &amp;&amp; (place_free(x-4,y))){
        x-=2;
        sprite_index = spr_Player_Walk_Left; 
        image_speed = 0.5;
        Direction = "Left";
    }
    if(!keyboard_check(ord("D")) &amp;&amp; !keyboard_check(ord("A")) &amp;&amp; !keyboard_check(ord("S")) &amp;&amp;!keyboard_check(ord("W"))){
        if(Direction = "Down"){
           sprite_index = spr_Player_Down;
        }
        if(Direction = "Up"){
            sprite_index = spr_Player_Up;
        }
        if(Direction = "Left"){
            sprite_index = spr_Player_Left;
        }
        if(Direction = "Right"){
            sprite_index = spr_Player_Right;
        }
   }
}
